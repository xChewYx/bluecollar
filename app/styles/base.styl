================================================================== */
.testimonial-block {
  border: 1px solid #e3e3e3;
  text-align: center;
  padding: 50px 180px;
}
.testimonial-block p.testimonial {
  color: #000;
  font-size: 22px;
  font-style: italic;
  line-height: 1.7em;
  margin-bottom: 10px;
  font-family: "PT Sans", Helvetica, Arial, sans-serif;
}
