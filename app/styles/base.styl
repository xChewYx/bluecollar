.pricing-table ul.table-features li:nth-child(even) {
  background: #f6f6f6;
}
.pricing-table a.button {
  width: 150px;
  font-size: 16px;
  padding: 3px 4px;
  margin-bottom: 20px;
  margin-top: 70px;
}
.pricing-table.featured a.button {
  margin-top: 100px;
}
